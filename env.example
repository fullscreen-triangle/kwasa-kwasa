# Kwasa-Kwasa Environment Configuration
# Copy this file to .env and customize the values for your environment

# Application Configuration
KWASA_ENV=development
RUST_LOG=info
RUST_BACKTRACE=1

# Server Configuration
HOST=127.0.0.1
PORT=8080
WORKERS=4

# Database Configuration
DATABASE_URL=sqlite:./data/kwasa.db
POSTGRES_URL=postgresql://kwasa:kwasa_password@localhost:5432/kwasa_kwasa
REDIS_URL=redis://localhost:6379

# Security Configuration
SECRET_KEY=your-secret-key-here-change-this-in-production
JWT_SECRET=your-jwt-secret-here
SESSION_TIMEOUT=3600

# API Keys and External Services
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
GOOGLE_API_KEY=your-google-api-key

# Scientific Computing APIs
PUBCHEM_API_KEY=your-pubchem-api-key
NCBI_API_KEY=your-ncbi-api-key
UNIPROT_API_KEY=your-uniprot-api-key

# Autobahn Reasoning Engine Configuration
AUTOBAHN_ENDPOINT=http://localhost:8081
AUTOBAHN_API_KEY=your-autobahn-api-key
AUTOBAHN_TIMEOUT=30

# Metacognitive Orchestrator Configuration
META_ORCHESTRATOR_ENABLED=true
META_ORCHESTRATOR_ENDPOINT=http://localhost:8082
META_ORCHESTRATOR_API_KEY=your-meta-orchestrator-api-key

# Specialized Module Configuration
CHEMINFORMATICS_ENABLED=false
SYSTEMS_BIOLOGY_ENABLED=false
SPECTROMETRY_ENABLED=false
MULTIMEDIA_ENABLED=false

# Performance Configuration
MAX_WORKERS=8
CACHE_SIZE=1000
MEMORY_LIMIT=2GB
PROCESSING_TIMEOUT=300

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json
LOG_FILE=./logs/kwasa.log
LOG_ROTATION=daily
LOG_MAX_SIZE=100MB

# Monitoring Configuration
METRICS_ENABLED=true
METRICS_PORT=9090
HEALTH_CHECK_ENABLED=true
TELEMETRY_ENABLED=false

# Development Configuration
HOT_RELOAD=true
DEBUG_MODE=false
PROFILE_ENABLED=false

# Storage Configuration
DATA_DIR=./data
TEMP_DIR=./tmp
UPLOAD_DIR=./uploads
MAX_UPLOAD_SIZE=100MB

# Network Configuration
CORS_ENABLED=true
CORS_ORIGINS=*
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# WebAssembly Configuration
WASM_ENABLED=false
WASM_MEMORY_LIMIT=64MB

# Scientific Computing Configuration
SCIENTIFIC_PRECISION=double
NUMERICAL_TOLERANCE=1e-10
PARALLEL_PROCESSING=true
GPU_ACCELERATION=false

# Turbulance Language Configuration
TURBULANCE_STRICT_MODE=false
TURBULANCE_MAX_RECURSION=1000
TURBULANCE_MEMORY_LIMIT=1GB

# Feature Flags
EXPERIMENTAL_FEATURES=false
BETA_FEATURES=false
ADVANCED_ANALYTICS=true
REAL_TIME_PROCESSING=true

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_INTERVAL=daily
BACKUP_RETENTION=30
BACKUP_LOCATION=./backups

# Email Configuration (for notifications)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USERNAME=your-smtp-username
SMTP_PASSWORD=your-smtp-password
FROM_EMAIL=noreply@kwasa-kwasa.example.com

# Webhook Configuration
WEBHOOK_ENABLED=false
WEBHOOK_URL=https://your-webhook-endpoint.com
WEBHOOK_SECRET=your-webhook-secret 